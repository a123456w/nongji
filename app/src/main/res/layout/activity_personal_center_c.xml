<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.guo.qlzx.nongji.client.activity.SPersonalCentercActivity">

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#efefef"></View>

    <LinearLayout
        android:id="@+id/ll_dingdan"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="10dp"
        android:background="#fff"
        android:gravity="center_vertical">

        <ImageView
            android:layout_width="18dp"
            android:layout_height="22dp"
            android:layout_marginBottom="9dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="11dp"
            android:layout_marginTop="9dp"
            android:src="@drawable/ic_dingdan" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="17dp"
            android:gravity="center_vertical"
            android:text="订单"
            android:textColor="#333333"
            android:textSize="12sp" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_zuoyejilu"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="10dp"
        android:background="#fff"
        android:gravity="center_vertical">

        <ImageView
            android:layout_width="18dp"
            android:layout_height="22dp"
            android:layout_marginBottom="9dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="11dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_zuoyejilu" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="17dp"

            android:gravity="center_vertical"
            android:text="作业记录"
            android:textColor="#333333"
            android:textSize="12sp" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_qianbao"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="10dp"
        android:background="#fff"
        android:gravity="center_vertical">

        <ImageView
            android:layout_width="18dp"
            android:layout_height="22dp"
            android:layout_marginBottom="9dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="11dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/ic_qianbao" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="17dp"

            android:gravity="center_vertical"
            android:text="钱包"
            android:textColor="#333333"
            android:textSize="12sp" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_youhuijuan"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="10dp"
        android:background="#fff"
        android:gravity="center_vertical">

        <ImageView
            android:layout_width="18dp"
            android:layout_height="22dp"
            android:layout_marginBottom="9dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="11dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_youhuijuan" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="17dp"
            android:gravity="center_vertical"
            android:text="优惠卷码兑换"
            android:textColor="#333333"
            android:textSize="12sp" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_suishouji"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="10dp"
        android:background="#fff"
        android:gravity="center_vertical">

        <ImageView
            android:layout_width="18dp"
            android:layout_height="22dp"
            android:layout_marginBottom="9dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="11dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_suishouji" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="17dp"

            android:gravity="center_vertical"
            android:text="随手记"
            android:textColor="#333333"
            android:textSize="12sp" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_updatapaypassword"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="10dp"
        android:background="#fff"
        android:gravity="center_vertical">

        <ImageView
            android:layout_width="18dp"
            android:layout_height="22dp"
            android:layout_marginBottom="9dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="11dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/ic_updatapaypassword" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="17dp"

            android:gravity="center_vertical"
            android:text="更改支付密码"
            android:textColor="#333333"
            android:textSize="12sp" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="86dp"
        android:layout_marginTop="8dp"
        android:background="#fff"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/ll_updata_password"
            android:layout_width="match_parent"
            android:layout_height="41dp"
            android:layout_gravity="center_vertical"
            android:background="#fff"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="18dp"
                android:layout_height="22dp"
                android:layout_marginBottom="9dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="11dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/ic_updatapassword" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="17dp"

                android:gravity="center_vertical"
                android:text="更改登录密码"
                android:textColor="#333333"
                android:textSize="12sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="16dp"
            android:background="#efefef"></LinearLayout>


        <LinearLayout
            android:id="@+id/ll_finshlogin"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_gravity="center_vertical"
            android:background="#fff"
            android:gravity="center_vertical">

            <ImageView
                android:layout_width="18dp"
                android:layout_height="22dp"
                android:layout_marginBottom="9dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="11dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/ic_finshlogin" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="17dp"
                android:gravity="center_vertical"

                android:text="退出登录"
                android:textColor="#333333"
                android:textSize="12sp" />

        </LinearLayout>


    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_yjzx"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="10dp"
        android:background="#fff"
        android:gravity="center_vertical">

        <ImageView
            android:layout_width="18dp"
            android:layout_height="22dp"
            android:layout_marginBottom="9dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="11dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_zhuxiao" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="17dp"
            android:gravity="center_vertical"
            android:text="永久注销账号"
            android:textColor="#333333"
            android:textSize="12sp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:gravity="center">

        <TextView
            android:id="@+id/tv_feedback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="意见反馈"
            android:textColor="@color/textcolor9"
            android:layout_marginRight="10dp"
            android:textSize="12sp" />
        <View
            android:layout_width="1dp"
            android:layout_height="15dp"
            android:background="@color/textcolor9"></View>
        <TextView
            android:id="@+id/tv_ver_name"
            android:layout_width="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:text="版本号"
            android:textColor="@color/textcolor9"
            android:textSize="12sp" />
    </LinearLayout>

</LinearLayout>
