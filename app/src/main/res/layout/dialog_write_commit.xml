<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="180dp"
    android:layout_marginBottom="252dp"
    android:layout_marginLeft="35dp"
    android:layout_marginTop="239dp"
    android:background="@drawable/shap_yuanjiaos"
    android:orientation="vertical">

    <ImageView
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="19dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_com"
        android:text="退出登录"
        android:textColor="#333333"
        android:textSize="15sp" />
    <TextView
        android:id="@+id/tv_sn"
        android:layout_marginBottom="20dp"
        android:layout_gravity="center"
        android:textColor="#333333"
        android:textSize="14sp"
        android:text="是否确定A99正常提交？"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:layout_width="296dp"
        android:layout_height="38dp"
        android:orientation="horizontal">
        <Button
            android:layout_marginLeft="25dp"
            android:textColor="@color/red"
            android:textSize="15sp"
            android:id="@+id/but_ca"
            android:background="@drawable/shape_gray_red"
            android:layout_width="60dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:text="取消" />
        <Button
            android:layout_marginLeft="88dp"
            android:layout_marginRight="25dp"
            android:textSize="15sp"
            android:textColor="#ffffff"
            android:id="@+id/but_co"
            android:background="@drawable/shape_red_three"
            android:layout_width="60dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:text="确定" />

    </LinearLayout>

</LinearLayout>
